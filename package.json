{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "db:migrate:dev": "turbo run db:migrate:dev",
    "db:migrate:deploy": "turbo run db:migrate:deploy",
    "db:push": "turbo run db:push",
    "db:seed": "turbo run db:seed",
    "db:clean": "turbo run db:clean",
    "db:generate": "turbo run db:generate",
    "dev": "turbo run dev",
    "format": "prettier --write .",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "docker-compose": "turbo run docker-compose",
    "clean": "turbo run clean && rimraf .turbo node_modules dist",
    "setup": "tsx set-env-variables.ts && turbo run docker-compose && turbo run db:push && turbo run db:seed",
    "terraform:init": "turbo run terraform:init",
    "terraform:plan": "turbo run terraform:plan",
    "terraform:apply": "turbo run terraform:apply"
  },
  "devDependencies": {
    "@mr/config": "workspace:*",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "tsx": "^3.14.0",
    "turbo": "^1.10.16"
  },
  "packageManager": "pnpm@8.6.12"
}
